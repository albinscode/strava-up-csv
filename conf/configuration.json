                "default": null,
                "header": "Elapsed time"
            },
            "total_elevation_gain": {
                "filter": null,
                "default": "",
                "header": "D+"
            },
            "type": {
                "filter": null,
                "default": "",
                "header": "Type"
            },
            "workout_type": {
                "filter": null,
                "default": "",
                "header": "Workout type"
            },
            "id": {
                "filter": null,
                "default": "",
                "header": "Id"
            },
            "start_date_local": {
                "filter": null,
                "default": "",
                "header": "Date"
            },
            "average_speed": {
                "filter": "convertSpeedToKmh",
                "default": "",
                "header": "Average speed"
            }
        },
        "column_separator": "\t",
